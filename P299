#include <iostream>
#include <vector>
using namespace std;

int contar(){
    int a;int b;int c;int aux=0;int cont=0;
    cin>>a;
    vector <int> v;
    vector <int> v2;
    vector <int> v3;
    int limite = (sizeof(v)/sizeof(v[0]));
    cout<<endl;

    for (int i=0;i<a;i++){
        cin>>b;

        for(int j=0;j<b;j++){
            cin>>c;
            v.push_back(c);
            aux=aux+1;
        }

        for(int u=cont;u<aux;u++){
            cout<<v[cont]<<" ";

        }
        cont=aux;


        cout<<endl;
    }








}










int main() {
    cout<<contar();

}
